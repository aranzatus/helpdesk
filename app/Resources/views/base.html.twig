<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Helpdesk!{% endblock %}</title>
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.11.3.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% endblock %}
        {% block stylesheets %}
        <link href="{{ asset('bootstrap/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('csspersonalizado/styles.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="pa">
        <nav class="navbar navbar-default">
        <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="{{ path('helpdesk_inicio_soporte')}}">HELPDESK</a>
    </div>
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Solicitudes <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><a href="{{ path('helpdesk_solicitudes_listar')}}">Listar</a></li>
            <li><a href="{{ path('helpdesk_solicitudes_nuevo')}}">Nuevo</a></li>
            <li><a href="#">Otro</a></li>
          </ul>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Tipos Solicitudes <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><a href="{{ path('helpdesk_solicitudestipos_listar')}}">Listar</a></li>
            <li><a href="{{ path('helpdesk_solicitudestipos_nuevo')}}">Nuevo</a></li>
            <li><a href="#">Otro</a></li>
          </ul>
        </li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Usuarios <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
            <li><a href="{{ path('helpdesk_usuarios_listar')}}">Listar</a></li>
            <li><a href="{{ path('helpdesk_usuarios_nuevo')}}">Nuevo</a></li>
            <li><a href="#">Otro</a></li>
          </ul>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><a href="#">Salir</a></li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
    {% block body %}{% endblock %}
    </div>   
    </body>
</html>
